    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "symfony/yaml": "~2.1",
        "satooshi/php-coveralls": "~1.0",
        "symfony/phpunit-bridge": "^3.1"
    },
    "authors": [
        {
            "name": "Mikael Peigney",
            "email": "mikael.peigney@supinfo.com"
        }
    ],
    "require": {
